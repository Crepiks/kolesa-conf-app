<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingBottom="@dimen/item_branch_events_header_padding_bottom"
    >

    <TextView
        android:id="@+id/item_branch_events_header_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/item_branch_events_header_title_color"
        android:textSize="@dimen/item_branch_events_header_title_size"
        android:textStyle="bold"
        tools:text="Android"
        />

</LinearLayout>